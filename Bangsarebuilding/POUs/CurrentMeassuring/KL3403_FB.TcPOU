	arrPowerMeasure: ARRAY [0..11] OF INT;
	realCurrentRMS: REAL;
	realVoltageRMS: REAL;
	realEff_Power: REAL;
	realPF: REAL;
	realEnergyConsumtion: REAL;
	realPeakCurrent: REAL;
