  <POU Name="KL3403_FB" Id="{475f268e-7c86-48e9-9772-e63f91e0c672}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK KL3403_FB
VAR
	TP1: TP;
	TP2: TP;
	R_TRIG1: R_TRIG;

	INDEX: BYTE;
	CurrentRMS: REAL;
	VoltageRMS: REAL;
	EffectivePower: REAL;
	PowerFactor: REAL;
	EnergyConsuntion: REAL;
	PeakCurrent: REAL;
	PeakVoltage: REAL;
	PeakPower: REAL;
	Frequency: REAL;
